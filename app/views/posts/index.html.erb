<h1>Posts</h1>
<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Description</th>
    <th>Actions</th>
  </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.id %></td>
    <td><%= post.title %></td>
    <td><%= post.description %></td>
    <td>actions,edit, bla, bla</td>
  </tr>
<% end %>
</table>
